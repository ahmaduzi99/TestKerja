@{
	Layout = "_Nolayout";
}

@model List<Registrasi>

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- Boxicons -->
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
	<!-- My CSS -->
	<link rel="stylesheet" href="~/css/stylee.css">

	<title>AdminHub</title>
</head>
<body>


	<!-- SIDEBAR -->
	<section id="sidebar">
		<a href="#" class="brand">
			<i class='bx bxs-smile'></i>
			<span class="text">AdminHub</span>
		</a>
		<ul class="side-menu top">
			<li class="active">
				<a href="/dashboard/index">
					<i class='bx bxs-dashboard'></i>
					<span class="text">Dashboard</span>
				</a>
			</li>
			<li>
				<a href="/registrasi/index">
					<i class='bx bxs-shopping-bag-alt'></i>
					<span class="text">Agen Form</span>
				</a>
			</li>
			<li>
				<a href="/lapangan/index">
					<i class='bx bxs-message-dots'></i>
					<span class="text">No 3</span>
				</a>
			</li>
		</ul>
	</section>
	<!-- SIDEBAR -->
	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
			<i class='bx bx-menu'></i>
			<a href="#" class="nav-link">Categories</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search'></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="#" class="notification">
				<i class='bx bxs-bell'></i>
				<span class="num">8</span>
			</a>
			<a href="#" class="profile">
				<img src="img/people.png">
			</a>
		</nav>
		<!-- NAVBAR -->
		<!-- MAIN -->
		<main>
			<div class="head-title">
				<div class="left">
					<h1>Form Agen</h1>
					<ul class="breadcrumb">
						<li>
							<a href="#">Dashboard</a>
						</li>
						<li><i class='bx bx-chevron-right'></i></li>
						<li>
							<a class="active" href="#">Home</a>
						</li>
					</ul>
				</div>

			</div>

			<ul class="box-info">
			</ul>


			<div class="table-data">
				<div class="order">
					<div class="head">
						<h3>List Agen yang sudah Registrasi</h3>
						<a href="/registrasi/create" class="btn btn-warning">Create</a>
						<i class='bx bx-search'></i>
						<i class='bx bx-filter'></i>
					</div>
					<table>
						<thead>
							<tr>
								<th>Nama</th>
								<th>Addres</th>
								<th>Phone</th>
								<th>Status</th>
								<th>Action</th>
							</tr>
						</thead>
						<tbody>
							@foreach (var registrasi in Model)
							{
								<tr>
									<td>

										<p>@registrasi.Name</p>
									</td>
									<td>@registrasi.Adress</td>
									<td>@registrasi.Phone</td>
									<td>@registrasi.RegStatus</td>
									<td>
										<a href="/registrasi/detail/@registrasi.Id" class="btn btn-success">Detail</a>
										<a href="/registrasi/delete/@registrasi.Id" class="btn btn-danger">Delete</a>
										<a href="/registrasi/edit/@registrasi.Id" class="btn btn-warning">Edit</a>
									</td>

								</tr>
							}


						</tbody>
					</table>
				</div>
			</div>
		</main>
		<!-- MAIN -->
	</section>
	<!-- CONTENT -->


	<script src="~/js/scriptbaru.js"></script>
</body>
</html>