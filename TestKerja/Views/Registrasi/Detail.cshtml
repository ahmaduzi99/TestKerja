@model Registrasi
@{
    Layout = "_Nolayout";
}

<div class="row">
    <div class="col-6">
        <div class="card">
            <div class="card-header">
                <h3>Detail Yang Sudah Registrasi</h3>
            </div>
            <div class="card-body">
                <table class="table table-bordered">
                    <tbody>
                        <tr>
                            <td>Id Reg</td>
                            <td>@Model.Id</td>
                        </tr>
                        <tr>
                            <td>Tanggal Registrasi</td>
                            <td>@Model.RegDate</td>
                        </tr>
                        <tr>
                            <td>Status</td>
                            <td>@Model.RegStatus</td>
                        </tr>
                        <tr>
                            <td>Nama</td>
                            <td>@Model.Name</td>
                        </tr>
                         <tr>
                            <td>Jenis Kelamin</td>
                            <td>@Model.gender</td>
                        </tr>
                        <tr>
                            <td>Tanggal Lahir</td>
                            <td>@Model.BirthPlace</td>
                        </tr>
                        <tr>
                            <td>Alamat</td>
                            <td>@Model.Adress</td>
                        </tr>
                        <tr>
                            <td>Email</td>
                            <td>@Model.Email</td>
                        </tr>
                         <tr>
                            <td>No Telp</td>
                            <td>@Model.Phone</td>
                        </tr>
                        <tr>
                            <td>ID Card</td>
                            <td>@Model.IdCard</td>
                        </tr>
                        <tr>
                            <td>Tanggal Buat</td>
                            <td>@Model.CreateDate</td>
                        </tr>
                        <tr>
                            <td>Di buat oleh</td>
                            <td>@Model.CreateBy</td>
                        </tr>
                       
                        <tr>
                            <td>Action</td>
                            <td><a class="btn btn-danger" href="/registrasi/index"> Back</a></td>

                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>